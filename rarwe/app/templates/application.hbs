<div class="container">
  <div class="page-header">
    <h1>Rock and Roll
      <small>with Emberjs</small>
    </h1>


  </div>

  <div class="row">
    <div class="col-md-6">
      <ul class="list-group songs">
        {{#each model as |song|}}
        <li class="list-group-item">{{song.title}} by {{song.band}} ({{song.rating}})</li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>